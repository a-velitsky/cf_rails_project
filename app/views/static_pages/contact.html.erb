<div class="container">
  <h1>Contact Us</h1>
  <h3>Phone Number: 718-753-3770</h2>
  <h3>Email: a.velitsky@gmail.com</h2>
  <h3>LinkedIn: https://www.linkedin.com/in/aleksandrvelitsky</h2>
  <h3>Skype: a.velitsky@gmail.com</h2>
  <%= flash[:notice] %>
  <%= form_tag("/static_pages/thank_you") do %>
    <div class="row">
      <div class="col-md-4">
        <div class="form-group">
          <%= label_tag :name %>
          <%= text_field_tag :name, nil, class: "form-control", placeholder: "Enter Name" %>
        </div>      
        <div class="form-group">
          <%= label_tag :email, "Email Address" %>
          <%= text_field_tag :email, nil, class: "form-control", placeholder: "Enter Email" %>
        </div>
        <div class="form-group">
          <%= label_tag :message %>
          <%= text_area_tag :message, nil, class: "form-control", placeholder: "Enter Message" %>
        </div>      
      </div>
    </div>
    <%= submit_tag "Send" %>   
  <% end %>
</div>